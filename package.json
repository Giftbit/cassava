{
  "name": "cassava",
  "version": "2.5.2",
  "description": "AWS API Gateway Router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "docs": "rimraf ./docs && typedoc src/ --out docs --mode modules --module commonjs --target es6 --excludePrivate --exclude \"**/*.test.ts\" --name Cassava && touch ./docs/.nojekyll && gh-pages -d docs -t",
    "lint": "eslint . --ext .ts,.tsx",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test && npm run docs",
    "test": "mocha --recursive --throw-deprecation --require ts-node/register \"src/**/*.test.ts\"",
    "test:watch": "npm run test -- --watch --watch-extensions ts,tsx --growl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/cassava.git"
  },
  "keywords": [
    "routing",
    "apigateway",
    "lambda",
    "serverless",
    "aws",
    "cassava"
  ],
  "contributors": [
    "Jeffery Grajkowski <pushplay@gmail.com> (https://github.com/pushplay/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/cassava/issues"
  },
  "homepage": "https://github.com/Giftbit/cassava#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.50",
    "@types/chai": "^4.2.11",
    "@types/cookie": "^0.3.3",
    "@types/mocha": "^7.0.2",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^10.12.18",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "gh-pages": "^2.2.0",
    "mocha": "^7.1.1",
    "rimraf": "^3.0.2",
    "touch": "^3.1.0",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "cookie": "^0.4.0",
    "jsonschema": "^1.2.6",
    "negotiator": "^0.6.2",
    "uuid": "^7.0.3"
  }
}
