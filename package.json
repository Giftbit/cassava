{
  "name": "cassava",
  "version": "0.0.9",
  "description": "AWS API Gateway Router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "lint": "tslint --project tsconfig.json",
    "prepublish": "npm run clean && npm run build && npm run lint && npm run test",
    "test": "mocha --recursive --throw-deprecation --require ts-node/register \"src/**/*.test.ts\"",
    "test:watch": "npm run test -- --watch --watch-extensions ts,tsx --growl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/cassava.git"
  },
  "keywords": [
    "routing",
    "apigateway",
    "lambda",
    "serverless",
    "aws",
    "cassava"
  ],
  "contributors": [
    "Jeffery Grajkowski <pushplay@gmail.com> (https://github.com/pushplay/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/cassava/issues"
  },
  "homepage": "https://github.com/Giftbit/cassava#readme",
  "devDependencies": {
    "@types/aws-lambda": "^0.0.8",
    "@types/chai": "^3.4.35",
    "@types/cookie": "^0.3.0",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.8",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "ts-node": "^2.1.0",
    "tslint": "^4.5.1",
    "typescript": "^2.2.1"
  },
  "dependencies": {
    "cookie": "^0.3.1"
  }
}
