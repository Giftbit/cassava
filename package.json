{
  "name": "cassava",
  "version": "1.3.0",
  "description": "AWS API Gateway Router",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "docs": "rimraf ./docs && typedoc src/ --out docs --mode modules --module commonjs --target es6 --excludePrivate --exclude \"**/*.test.ts\" --name Cassava && touch ./docs/.nojekyll && gh-pages -d docs -t",
    "lint": "tslint --project tsconfig.json",
    "prepublishOnly": "npm run clean && npm run build && npm run lint && npm run test && npm run docs",
    "test": "mocha --recursive --throw-deprecation --require ts-node/register \"src/**/*.test.ts\"",
    "test:watch": "npm run test -- --watch --watch-extensions ts,tsx --growl"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Giftbit/cassava.git"
  },
  "keywords": [
    "routing",
    "apigateway",
    "lambda",
    "serverless",
    "aws",
    "cassava"
  ],
  "contributors": [
    "Jeffery Grajkowski <pushplay@gmail.com> (https://github.com/pushplay/)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Giftbit/cassava/issues"
  },
  "homepage": "https://github.com/Giftbit/cassava#readme",
  "devDependencies": {
    "@types/aws-lambda": "^0.0.16",
    "@types/chai": "^4.0.4",
    "@types/cookie": "^0.3.1",
    "@types/mocha": "^2.2.42",
    "@types/node": "^8.0.27",
    "@types/uuid": "^3.4.2",
    "chai": "^4.1.2",
    "gh-pages": "^1.0.0",
    "mocha": "^3.5.0",
    "rimraf": "^2.6.2",
    "touch": "^3.1.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "uuid": "3.1.0"
  }
}
